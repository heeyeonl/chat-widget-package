{
  "name": "chat-widget",
  "version": "1.0.0",
  "description": "A React chat widget component for embedding chat functionality in web applications",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/components/ChatWidget.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/components/ChatWidget.css"
  },
  "scripts": {
    "build": "tsc && cp src/components/ChatWidget.css dist/components/",
    "prepare": "npm run build"
  },
  "keywords": [
    "chat",
    "widget",
    "react"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "typescript": "^5.0.0"
  },
  "sideEffects": false
}
